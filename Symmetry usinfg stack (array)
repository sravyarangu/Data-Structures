#include<stdio.h>
#include<string.h>
char stack[20];
int top=-1;
void push(char);
char pop();
void main()
{
    char str[20],ch,s[20],a[20];
    int l,i,j=0;
    printf("\nenter string:");
   scanf("%s",&str);
    l=strlen(str);
     for(i=0;i<l;i++)
    {
        a[i]=str[i];
    }
    for(i=0;i<l;i++)
    {
        push(str[i]);
    }
    for(i=0;i<l;i++)
    {
      
        ch=pop();
        if(a[i]==ch){
            j++;
        }
    }
        if(j==l)
        {
            printf("\nSYMMETRY\n");
        }
        else if(j!=l)
        {
            printf("\nUNSYMMETRY");
        }
    //}
}
void push(char c){
    
    top++;
    stack[top]=c;
}
char pop()
{
    char c;
    c=stack[top];
    top--;
    return c;

}#include<stdio.h>
#include<string.h>
char stack[20];
int top=-1;
void push(char);
char pop();
void main()
{
    char str[20],ch,s[20],a[20];
    int l,i,j=0;
    printf("\nenter string:");
   scanf("%s",&str);
    l=strlen(str);
     for(i=0;i<l;i++)
    {
        a[i]=str[i];
    }
    for(i=0;i<l;i++)
    {
        push(str[i]);
    }
    for(i=0;i<l;i++)
    {
      
        ch=pop();
        if(a[i]==ch){
            j++;
        }
    }
        if(j==l)
        {
            printf("\nSYMMETRY\n");
        }
        else if(j!=l)
        {
            printf("\nUNSYMMETRY");
        }
    //}
}
void push(char c){
    
    top++;
    stack[top]=c;
}
char pop()
{
    char c;
    c=stack[top];
    top--;
    return c;

}
